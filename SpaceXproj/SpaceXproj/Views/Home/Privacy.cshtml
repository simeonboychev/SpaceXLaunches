@model IEnumerable<SpaceX>
@{
    ViewData["Title"] = "Privacy Policy";
}
@*<h1>@ViewData["Title"]</h1>*@

<style>
    table {
        /*table-layout:fixed;*/
    }

    table, th, td {
        border: 1.5px solid black;
        border-collapse: collapse;
    }

    th, td {
        padding: 5px;
    }

    td {
        height: 110px;
    }

    tr:nth-child(even) {
        background-color: #f2f2f2;
    }

    .test {
        text-align: center;
    }

    #criteria {
        background-image: url('/img/searchingicon.png'); /* Add a search icon to input */
        background-size: 20px;
        background-position: 10px 12px; /* Position the search icon */
        background-repeat: no-repeat; /* Do not repeat the icon image */
        width: 90%; /* Full-width */
        font-size: 16px; /* Increase font-size */
        padding: 12px 20px 12px 40px; /* Add some padding */
        border: 1px solid #ddd; /* Add a grey border */
        margin-bottom: 12px; /* Add some space below the input */
    }
</style>
<body>

    Search: <input id="criteria" type="text" placeholder="Search by mission name">
    <center>
        <button id="butoncheto" value="Submit" type="button" class="btn btn-primary btn-lg">Search</button>

    </center>

    <br />
    <table style="width: 100%">
        <tr>

            <th class="test" style="width:5%">Flight number</th>
            <th class="test" style="width:5%">Image</th>
            <th class="test" style="width:10%">Mission name</th>
            <th class="test" style="width:15%">Launch site</th>
            <th class="test" style="width:10%">Rocket name</th>
            <th class="test" style="width:10%">Date</th>
            <th class="test" style="width:40%">Details</th>
            <th class="test" style="width:5%">Download</th>
        </tr>
        @foreach (var item in @Model)
        {
            <tr>
                <td class="test">@item.Flight_number</td>
                <td class="test">
                    @if (@item.Links.Mission_Patch_Small != null)
                    {
                        <img style="width:50px" src="@item.Links.Mission_Patch_Small" />
                    }
                    else
                    {
                        <img style="width:50px" src="~/img/noImage.jpg" />
                    }
                </td>
                <td class="test">@item.Mission_name</td>
                <td class="test">@item.Launch_site.Site_name_long</td>
                <td class="test">@item.Rocket.Rocket_name</td>
                <td class="test">@item.Launch_date_utc</td>
                @if (item.Details == null)
                {
                    <td>No details for flight</td>
                }
                else
                {
                    <td style="overflow:scroll; text-wrap:normal;float:left; overflow-x: hidden; border:none">@item.Details</td>
                }
            <td>
                <div align="center">
                    <img style="width:50px; align-items:center;" src="data:image/png;base64," style="width:50px; align:middle" ; />
                </div>

            </td>
            </tr>
        }
    </table>
</body>

